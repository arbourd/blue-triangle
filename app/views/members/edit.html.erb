<h1>Update Your Profile</h1>
<%= form_for(@member, class: 'form-group') do |f| %>
  <div class="form-group row">
    <%= f.label 'Email', class: 'col-sm-2 form-control-label' %>
    <div class="col-sm-10 form-control-label">
      <strong><%= @member.email%></strong>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label 'First name', class: 'col-sm-2 form-control-label' %>
    <div class="col-sm-10">
      <%= f.text_field :fname, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label 'Last name', class: 'col-sm-2 form-control-label' %>
    <div class="col-sm-10">
      <%= f.text_field :lname, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-offset-2 col-sm-10">
      <%= f.submit 'Update Profile', class: 'btn btn-primary-outline' %>
    </div>
  </div>
<% end %>
