<script>var topMenu = document.getElementById("cssmenu")
topMenu.hidden = true;
</script>
  <br>

<h2 class="deetHead2">Login</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<table id="tblDeets2">
  <tr>
    <th><%= f.label 'email:' %></th>
    <td><%= f.email_field :email, autofocus: true, size:"35" %></td>
</tr>
  <tr>
    <th><%= f.label 'password:' %></th>
    <td><%= f.password_field :password, autocomplete: 'off', size:"35" %></td>
</tr>
  <% if devise_mapping.rememberable? -%>
  <tr>
    <td colspan="2">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
      </td>
</tr>
  <% end -%>
</table>
<br>
  <div class="actions">
    <%= f.submit 'Log in', class:"btype1" %>
  </div>
<% end %>

<p class="links"><%= render 'devise/shared/links' %></p>
